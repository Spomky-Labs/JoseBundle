{
    "name": "spomky-labs/jose-bundle",
    "type": "symfony-bundle",
    "description": "Symfony2 Jose Bundle",
    "keywords": ["JWS", "JWT", "JWE", "JWA", "JWK", "JWKSet", "Jose", "Jot", "Bundle", "Symfony2"],
    "homepage": "https://github.com/Spomky-Labs/JoseBundle",
    "license": "MIT",
    "authors": [
        {
          "name": "Florent Morselli"
        }
    ],
    "require": {
        "spomky-labs/jose": "^3.0@beta",
        "symfony/http-kernel": "^2.7|^3.0",
        "symfony/dependency-injection": "^2.7|^3.0",
        "symfony/config": "^2.7|^3.0",
        "fgrosse/phpasn1": "dev-compat/php5-5 as v1.3.1"
    },
    "require-dev": {
        "symfony/var-dumper": "^2.7|^3.0",
        "behat/behat": "^3.0",
        "behat/mink": "*",
        "behat/mink-extension": "^2.1",
        "behat/mink-browserkit-driver":  "*",
        "behat/symfony2-extension":  "^2.1",
        "symfony/monolog-bundle": "^2.10",
        "ext-curl": "*"
    },
    "autoload": {
        "psr-4": {
            "SpomkyLabs\\JoseBundle\\": ""
        }
    },
    "autoload-dev": {
        "psr-4": {
            "SpomkyLabs\\TestJoseBundle\\": "Tests/TestJoseBundle",
            "SpomkyLabs\\JoseBundle\\Features\\Context\\": "Tests/Context"
        }
    },
    "extra": {
        "branch-alias": {
            "dev-master": "1.0.x-dev"
        }
    }
}

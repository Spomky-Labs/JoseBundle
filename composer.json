{
    "name": "spomky-labs/jose-bundle",
    "type": "symfony-bundle",
    "description": "Symfony2 Jose Bundle",
    "keywords": ["JWS", "JWT", "JWE", "JWA", "JWK", "JWKSet", "Jose", "Jot", "Bundle", "Symfony2"],
    "homepage": "https://github.com/Spomky-Labs/JoseBundle",
    "license": "MIT",
    "authors": [
        {
          "name": "Florent Morselli"
        }
    ],
    "require": {
        "php": ">=5.6",
        "symfony/framework-bundle": "~2.7",
        "spomky-labs/jose": "~0.1.0"
    },
    "require-dev": {
        "symfony/monolog-bundle": "~2.7",
        "sensio/framework-extra-bundle": "^3.0",
        "symfony/symfony": "~2.7",
        "behat/behat": "3.*",
        "behat/symfony2-extension": "2.0.x-dev",
        "behat/mink": "*",
        "behat/mink-extension": "*",
        "behat/mink-browserkit-driver":  "*",
        "doctrine/doctrine-fixtures-bundle": "~2.2",
        "vipsoft/doctrine-data-fixtures-extension": "dev-master",
        "doctrine/orm": ">=2.2"
    },
    "suggest": {
    },
    "autoload": {
        "psr-4": {
            "SpomkyLabs\\JoseBundle\\": ""
        }
    },
    "autoload-dev": {
        "psr-4": {
            "SpomkyLabs\\TestJoseBundle\\": "Tests/TestJoseBundle"
        }
    },
    "extra": {
        "branch-alias": {
            "dev-master": "0.0.x-dev"
        }
    }
}

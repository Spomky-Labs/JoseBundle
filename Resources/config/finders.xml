<?xml version="1.0" ?>

<container xmlns="http://symfony.com/schema/dic/services"
           xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
           xsi:schemaLocation="http://symfony.com/schema/dic/services http://symfony.com/schema/dic/services/services-1.0.xsd">

    <services>
        <service id="jose.finder.jwk" class="Jose\Finder\JWKFinder" public="false">
            <tag name="jose_jwk_finder" />
        </service>
        <service id="jose.finder.x5c" class="Jose\Finder\X5CFinder" public="false">
            <tag name="jose_jwk_finder" />
        </service>

        <service id="jose.finder.jwu" class="Jose\Finder\JKUFinder" public="false">
            <tag name="jose_jwk_finder" />
        </service>
        <service id="jose.finder.x5u" class="Jose\Finder\X5UFinder" public="false">
            <tag name="jose_jwk_finder" />
        </service>

        <service id="jose.finder.alg" class="SpomkyLabs\JoseBundle\Finder\AlgorithmFinder" public="false">
            <argument type="service" id="jose.key_storage" />
            <tag name="jose_jwk_finder" />
        </service>
        <service id="jose.finder.kid" class="SpomkyLabs\JoseBundle\Finder\KIDFinder" public="false">
            <argument type="service" id="jose.key_storage" />
            <tag name="jose_jwk_finder" />
        </service>
    </services>
</container>

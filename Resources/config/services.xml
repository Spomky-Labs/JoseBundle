<?xml version="1.0" ?>

<container xmlns="http://symfony.com/schema/dic/services"
    xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
    xsi:schemaLocation="http://symfony.com/schema/dic/services http://symfony.com/schema/dic/services/services-1.0.xsd">

    <services>
        <service id="jose.factory.compression_manager" class="Jose\Factory\CompressionManagerFactory" />
        <service id="jose.factory.checker_manager" class="Jose\Factory\CheckerManagerFactory" />
        <service id="jose.factory.decrypter" class="Jose\Factory\DecrypterFactory" />
        <service id="jose.factory.encrypter" class="Jose\Factory\EncrypterFactory" />
        <service id="jose.factory.jwe" class="Jose\Factory\JWEFactory" />
        <service id="jose.factory.jwk" class="Jose\Factory\JWKFactory" />
        <service id="jose.factory.jws" class="Jose\Factory\JWSFactory" />
        <service id="jose.factory.signer" class="Jose\Factory\SignerFactory" />
        <service id="jose.factory.verifier" class="Jose\Factory\VerifierFactory" />
        <service id="jose.loader" class="Jose\Loader" />
    </services>
</container>

<?xml version="1.0" ?>

<container xmlns="http://symfony.com/schema/dic/services"
           xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
           xsi:schemaLocation="http://symfony.com/schema/dic/services http://symfony.com/schema/dic/services/services-1.0.xsd">

    <parameters>
        <parameter key="sl_jose.chain.algorithm.class">SpomkyLabs\JoseBundle\Chain\AlgorithmChain</parameter>
        <parameter key="sl_jose.chain.compression.class">SpomkyLabs\JoseBundle\Chain\CompressionChain</parameter>
    </parameters>

    <services>
        <service id="sl_jose.chain.algorithm" class="%sl_jose.chain.algorithm.class%">
            <argument type="service" id="logger" on-invalid="ignore" />
        </service>
        <service id="sl_jose.chain.compression" class="%sl_jose.chain.compression.class%">
        </service>
    </services>
</container>

<?xml version="1.0" ?>

<container xmlns="http://symfony.com/schema/dic/services"
           xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
           xsi:schemaLocation="http://symfony.com/schema/dic/services http://symfony.com/schema/dic/services/services-1.0.xsd">

    <parameters>
        <parameter key="spomky_jose.chain.algorithm.class">SpomkyLabs\JoseBundle\Chain\AlgorithmChain</parameter>
        <parameter key="spomky_jose.chain.compression.class">SpomkyLabs\JoseBundle\Chain\CompressionChain</parameter>
    </parameters>

    <services>
        <service id="spomky_jose.chain.algorithm" class="%spomky_jose.chain.algorithm.class%">
            <argument>%spomky_jose.algorithms%</argument>
        </service>
        <service id="spomky_jose.chain.compression" class="%spomky_jose.chain.compression.class%">
            <argument>%spomky_jose.compression_methods%</argument>
        </service>
    </services>
</container>

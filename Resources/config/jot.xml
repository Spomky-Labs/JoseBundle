<?xml version="1.0" ?>

<container xmlns="http://symfony.com/schema/dic/services"
    xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
    xsi:schemaLocation="http://symfony.com/schema/dic/services http://symfony.com/schema/dic/services/services-1.0.xsd">

    <services>
        <service id="jose.jot_manager.default" class="SpomkyLabs\JoseBundle\Model\JotManager">
            <argument>%jose.jot.class%</argument>
            <argument type="service" id="doctrine" />
        </service>

        <service id="jose.checker.jti" class="SpomkyLabs\JoseBundle\Checker\JtiChecker" public="false">
            <argument type="service" id="jose.jot.manager" />
            <tag name="jose_checker" />
        </service>
    </services>
</container>

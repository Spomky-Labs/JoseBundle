<?xml version="1.0" ?>

<container xmlns="http://symfony.com/schema/dic/services"
    xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
    xsi:schemaLocation="http://symfony.com/schema/dic/services http://symfony.com/schema/dic/services/services-1.0.xsd">

    <services>
        <service id="jose.jwk_source.file" class="SpomkyLabs\JoseBundle\DependencyInjection\Source\JWKSource\KeyFile">
            <tag name="jose.jwk_source" />
        </service>

        <service id="jose.jwk_source.certificate" class="SpomkyLabs\JoseBundle\DependencyInjection\Source\JWKSource\CertificateFile">
            <tag name="jose.jwk_source" />
        </service>

        <service id="jose.jwk_source.values" class="SpomkyLabs\JoseBundle\DependencyInjection\Source\JWKSource\Values">
            <tag name="jose.jwk_source" />
        </service>

        <service id="jose.jwk_source.jwk" class="SpomkyLabs\JoseBundle\DependencyInjection\Source\JWKSource\JWK">
            <tag name="jose.jwk_source" />
        </service>

        <service id="jose.jwk_source.x5c" class="SpomkyLabs\JoseBundle\DependencyInjection\Source\JWKSource\X5C">
            <tag name="jose.jwk_source" />
        </service>

        <service id="jose.jwk_source.random.oct" class="SpomkyLabs\JoseBundle\DependencyInjection\Source\JWKSource\RandomOctKey">
            <tag name="jose.jwk_source" />
        </service>

        <service id="jose.jwk_source.random.rsa" class="SpomkyLabs\JoseBundle\DependencyInjection\Source\JWKSource\RandomRSAKey">
            <tag name="jose.jwk_source" />
        </service>

        <service id="jose.jwk_source.random.okp" class="SpomkyLabs\JoseBundle\DependencyInjection\Source\JWKSource\RandomOKPKey">
            <tag name="jose.jwk_source" />
        </service>

        <service id="jose.jwk_source.random.ec" class="SpomkyLabs\JoseBundle\DependencyInjection\Source\JWKSource\RandomECKey">
            <tag name="jose.jwk_source" />
        </service>

        <service id="jose.jwk_source.random.none" class="SpomkyLabs\JoseBundle\DependencyInjection\Source\JWKSource\RandomNoneKey">
            <tag name="jose.jwk_source" />
        </service>
    </services>
</container>
